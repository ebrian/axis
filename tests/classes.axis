// sample base class
class base {

    pri n1;

    +() {
        // constructor
        this:n1 = 0;
        this:n2 = 0;
    }

    pri fn add(a,b,c,d,e,f,g) {
        return this:n1 + this:n2;
    }

}

// sample class that inherits from another class
class calculator -> base {

    pub fn math(n1, n2) {
        this:n1 = n1;
        this:n2 = n2;

        //echo this:add();
        //echo this:sub();
    }

    -() {
        // destructor
    }

}

// sample object instantiation
var calc = +calculator();
calc:math(1,2);

