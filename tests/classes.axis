class base {

    pri n1;
    pri n2;

    +() {
        // constructor
        this:n1 = 0;
        this:n2 = 0;
    }

    pri fn add() {
        return this:n1 + this:n2;
    }

    pri fn sub() {
        return this:n1 - this:n2;
    }

    -() {
        // destructor
    }

}

class calculator -> base {

    pub fn math(n1, n2) {
        this:n1 = n1;
        this:n2 = n2;

        echo this:add();
        echo this:sub();
    }

}

var calc = +calculator;
calc:math();

